     "output_type": "stream",
     "text": [
      "digite um número: 6\n",
      "o fatorial de 6 = 6 x 5 x 4 x 3 x 2 x 1 = \u001b[4;34m720\u001b[m\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"digite um número: \"))\n",
    "n1=n\n",
    "s=n\n",
    "x=\"\"\n",
    "while n>1:\n",
    "    x+=(f\" {n} x\")\n",
    "    s=s*(n-1)\n",
    "    n-=1\n",
    "print(f\"o fatorial de {n1} ={x} 1 = \\033[4;34m{s}\\033[m\")"
   ]
  }
